{
  "name": "agenteai",
  "version": "1.0.0",
  "description": "Agente AI Assistant",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:middleware": "jest src/tests/middleware.test.ts",
    "test:manual": "tsx src/scripts/test-middleware.ts",
    "test:auth": "tsx src/scripts/test-auth.ts",
    "test:e2e": "rimraf playwright-results playwright-reports && playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report playwright-reports",
    "test:e2e:smoke": "rimraf playwright-results playwright-reports && playwright test smoke.e2e.ts --headed",
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test:assistant": "tsx src/scripts/test-assistant.ts",
    "test:llm": "tsx src/scripts/test-llm-providers.ts",
    "test:chat": "tsx src/scripts/test-chat-auth.ts",
    "protect-lovable": "ts-node scripts/lovable-protection.ts",
    "verify-lovable": "ts-node scripts/lovable-verify.ts",
    "dev:lovable": "echo 'Open Lovable UI Builder'",
    "build:lovable": "ts-node scripts/lovable-build.ts"
  },
  "dependencies": {
