    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dot-case": "^4.0.0",
    "is-number": "^7.0.0",
    "lucide-react": "^0.471.0",
    "next": "14.1.0",
    "next-themes": "^0.4.4",
    "openai": "^4.78.1",
    "posthog-js": "^1.205.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.15.0",
    "redis": "4.6.12",
